@model IList<Park>
@{
    ViewData["Title"] = "National Park Geek";
}

@foreach (var park in Model)
{
<div class="flexcontainerhome">
    <div class="homeimagediv">
        <a asp-controller="home" asp-action="parkdetails" asp-route-parkcode="@park.ParkCode">
            <img src="~/images/parks/@(park.ParkCode).jpg" class="homeimage" />
        </a>
    </div>
    <div class="homeparkdesc">
        <h3>@park.ParkName</h3>
        <p>@park.ParkDescription</p>
    </div>
</div>


}
